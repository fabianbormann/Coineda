{
  "name": "coineda",
  "description": "Coineda aims to be a free to use, open source and local running crypto track and tax tool",
  "version": "0.2.0",
  "author": "Fabian Bormann <fabian.bormann@coineda.io>",
  "homepage": "./",
  "license": "GPL-3.0",
  "dependencies": {
    "@craco/craco": "^7.0.0-alpha.0",
    "@testing-library/user-event": "^13.2.1",
    "antd": "^4.16.13",
    "axios": "^0.26.0",
    "concurrently": "^7.0.0",
    "craco-less": "^2.0.0",
    "electron-is-dev": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "i18next": "^21.4.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-localstorage-cache": "^1.1.1",
    "idb": "^7.0.0",
    "moment": "^2.29.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-blockies": "^1.4.1",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.4",
    "react-jss": "^10.8.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^5.0.0",
    "recharts": "^2.1.9",
    "tslib": "^2.3.0",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.0",
    "xlsx": "^0.18.2"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "craco start",
    "frontend": "craco start",
    "build": "craco build",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^17.0.1",
    "electron-builder": "^22.14.13",
    "extend": ">=2.0.2",
    "immer": ">=9.0.6",
    "nth-check": ">=2.0.1",
    "set-value": ">=4.0.1"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "build": {
    "appId": "io.coineda.app",
    "mac": {
      "icon": "./build/icons/512x512.png",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "./build/icons/256x256.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "category": "Utility",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
