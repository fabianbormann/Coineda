{
  "name": "coineda",
  "description": "Coineda aims to be a free to use, open source and local running crypto track and tax tool",
  "version": "0.1.7",
  "author": "Fabian Bormann <fabian.bormann@coineda.io>",
  "homepage": "./",
  "license": "AGPL-3.0",
  "dependencies": {
    "@ant-design/charts": "^1.3.4",
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "antd": "^4.18.4",
    "axios": "^0.25.0",
    "bunyan": "^1.8.15",
    "chart.js": "^3.7.0",
    "coingecko-api": "^1.0.10",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "craco-less": "^1.20.0",
    "electron-is-dev": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.2",
    "express-fileupload": "^1.2.1",
    "geopattern": "^1.2.3",
    "i18next": "^21.6.6",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-localstorage-cache": "^1.1.1",
    "moment": "^2.29.1",
    "node-cache": "^5.1.2",
    "prettier": "^2.5.1",
    "react": "17.0.2",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "react-jss": "^10.9.0",
    "react-native": "0.67.1",
    "react-native-react-bridge": "^0.9.0",
    "react-native-webview": "^11.17.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^4.0.3",
    "recharts": "^2.1.8",
    "sqlite3": "^5.0.2",
    "tslib": "^2.3.0",
    "wait-on": "^6.0.0",
    "web-vitals": "^2.1.3",
    "xlsx": "^0.17.5"
  },
  "main": "public/electron.js",
  "scripts": {
    "android": "react-native start && react-native run-android",
    "ios": "react-native start && react-native run-ios",
    "prestart": "npm run server-dev &",
    "start": "craco start",
    "frontend": "craco start",
    "build": "craco build && cp -r ./src/server ./build",
    "test": "NODE_ENV=test jest",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "server": "node ./src/server/index.js",
    "server-dev": "nodemon ./src/server/index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "eslint": "7.14.0",
    "extend": ">=2.0.2",
    "immer": "^9.0.12",
    "metro-react-native-babel-preset": "^0.66.2",
    "nth-check": ">=2.0.1",
    "react-test-renderer": "17.0.2",
    "set-value": ">=4.0.1",
    "supertest": "^6.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "src/server",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "build": {
    "appId": "io.coineda.app",
    "mac": {
      "icon": "./build/icons/512x512.png",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "./build/icons/256x256.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "category": "Utility",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
