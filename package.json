{
  "name": "coineda",
  "description": "Coineda aims to be a free to use, open source and local running crypto track and tax tool",
  "version": "0.1.6",
  "author": "Fabian Bormann <fabian.bormann@coineda.io>",
  "homepage": "./",
  "license": "AGPL-3.0",
  "dependencies": {
    "@ant-design/charts": "^1.2.5",
    "@craco/craco": "^6.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "antd": "^4.16.13",
    "axios": "^0.24.0",
    "bunyan": "^1.8.15",
    "chart.js": "^3.6.0",
    "coingecko-api": "^1.0.10",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "craco-less": "^1.20.0",
    "eslint-plugin-prettier": "^4.0.0",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "geopattern": "^1.2.3",
    "i18next": "^21.4.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-localstorage-cache": "^1.1.1",
    "moment": "^2.29.1",
    "node-cache": "^5.1.2",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.3.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.4",
    "react-jss": "^10.8.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.3",
    "recharts": "^2.1.6",
    "sqlite3": "^5.0.2",
    "tslib": "^2.3.0",
    "wait-on": "^6.0.0",
    "web-vitals": "^2.1.0",
    "xlsx": "^0.17.2"
  },
  "main": "public/electron.js",
  "scripts": {
    "prestart": "npm run server-dev &",
    "start": "craco start",
    "frontend": "craco start",
    "build": "craco build && cp -r ./src/server ./build",
    "test": "NODE_ENV=test jest",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "server": "node ./src/server/index.js",
    "server-dev": "nodemon ./src/server/index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron-builder": "^22.9.1",
    "supertest": "^6.1.6",
    "electron": "^15.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "src/server",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "build": {
    "appId": "io.coineda.app",
    "mac": {
      "icon": "./build/icons/512x512.png",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "./build/icons/256x256.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "category": "Utility",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
